  RowBox[{"{", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"allT", "=", 
     RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"For", "[", 
     RowBox[{
      RowBox[{"i", "=", "1"}], ",", 
